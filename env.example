# Open Bank Project API Credentials
# Copy this file to .env and fill in your actual credentials
# Note: .env is already in .gitignore

# Base API configuration
OBP_BASE_URL=https://apisandbox.openbankproject.com
OBP_API_VERSION=v5.1.0
OBP_CONSUMER_ID=your_consumer_id_here

# DirectLogin credentials (for simple authentication)
OBP_USERNAME=your_username_here
OBP_PASSWORD=your_password_here
OBP_CONSUMER_KEY=your_consumer_key_here
OBP_CONSUMER_SECRET=your_consumer_secret_here
OBP_DIRECTLOGIN_ENDPOINT=https://apisandbox.openbankproject.com/my/logins/direct

# OAuth2 credentials (if using OAuth2 instead of DirectLogin)
OAUTH2_CLIENT_ID=your_oauth2_client_id_here
OAUTH2_REDIRECT_URI=http://127.0.0.1:8080/callback
OAUTH2_CLIENT_SCOPE=ReadAccountsBasic,ReadTransactionsBasic
OAUTH2_JWS_ALG=RS256
OAUTH2_JWK_PRIVATE_KEY=your_private_key_here

